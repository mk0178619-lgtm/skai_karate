{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "redirects": [
    {
      "source": "/(.*)",
      "has": [
        { "type": "host", "value": "skaikarate-kolathur.com" }
      ],
      "destination": "https://www.skaikarate-kolathur.com/:1",
      "permanent": true
    },
    {
      "source": "/index.html",
      "destination": "/",
      "permanent": true
    }
  ]
}
